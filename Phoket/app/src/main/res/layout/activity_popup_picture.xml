<FrameLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CC000000"
    tools:context="com.eattle.phoket.PopupPictureActivity">
<!--android:background="#AA000000" -->




    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.eattle.phoket.view.TouchImageView
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:scaleType="center"
            android:id="@+id/picture"/>
        <!-- Tag & Tag 추가란이 들어가는 Layout -->
        <FrameLayout
            android:id="@+id/tagLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom"></FrameLayout>
    </FrameLayout>

    <ImageView xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/exitStory"
        android:layout_margin="5dp"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:src="@mipmap/ic_close_white_24dp"
        android:layout_gravity="right|top" />
</FrameLayout>

